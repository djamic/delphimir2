procedure SendGameCenterMsg(wIdent: Word; sSENDMSG: string);
var
  SendData: TCopyDataStruct;
  nParam: Integer;
begin
  nParam := MakeLong(Word(tM2Server), wIdent);
  SendData.cbData := Length(sSENDMSG) + 1;
  GetMem(SendData.lpData, SendData.cbData);
  StrCopy(SendData.lpData, PChar(sSENDMSG));
  SendMessage(g_dwGameCenterHandle, WM_COPYDATA, nParam, Cardinal(@SendData));
  FreeMem(SendData.lpData);
end;

function GetIPAddr: string;
var
  A, B, C, D: Byte;
begin
  while True do begin
    A := Random(High(Byte));
    B := Random(High(Byte));
    C := Random(High(Byte));
    D := Random(High(Byte));
    if (A = 0) then Continue;
    if (A = 127) and (B = 0) and (C = 0) and (D = 1) then Continue;
    if (A = 192) and (B = 168) then Continue;
    if (A = 255) and (B = 255) then Continue;
    Result := Format('%d.%d.%d.%d', [A, B, C, D]);
    break;
  end;
end;

function GetIPLocal(sIPaddr: string): string;
var
  sLocal: array[0..254] of Char;
begin
  try
    if Assigned(PlugInEngine.IPLocal) then begin
      FillChar(sLocal, SizeOf(sLocal), 0);
      PlugInEngine.IPLocal(@sIPaddr[1], @sLocal, SizeOf(sLocal));
      Result := StrPas(PChar(sLocal[0]));
      Exit;
    end;
    Result := '未知！！！';
  except
    Result := '未知！！！';
  end;
end;

