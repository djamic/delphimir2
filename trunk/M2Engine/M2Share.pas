procedure LoadBoxItem(ItemBox: pTItemBox);
var
  I, nItemCount, nItemType: Integer;
  LoadList: TStringList;
  sFileName: string;
  sLineText, sItemType, sItemCount, sBoxName, sItemName: string;
  BoxItem: pTBoxItem;
begin
  sFileName := g_Config.sEnvirDir + 'ItemBox.txt';
  LoadList := TStringList.Create;
  if FileExists(sFileName) then begin
    try
      LoadList.LoadFromFile(sFileName);
    except
      LoadList.Clear;
    end;
    for I := 0 to LoadList.Count - 1 do begin
      sLineText := Trim(LoadList.Strings[I]);
      if sLineText = '' then Continue;
      if (sLineText <> '') and (sLineText[1] = ';') then Continue;
      sLineText := GetValidStr3(sLineText, sBoxName, [' ', ',', #9]);
      sLineText := GetValidStr3(sLineText, sItemName, [' ', ',', #9]);
      sLineText := GetValidStr3(sLineText, sItemType, [' ', ',', #9]);
      sLineText := GetValidStr3(sLineText, sItemCount, [' ', ',', #9]);
      nItemCount := Str_ToInt(sItemCount, -1);
      nItemType := Str_ToInt(sItemType, -1);
      if (sBoxName <> '') and (sItemName <> '') and (nItemType in [0..2]) and (CompareText(sBoxName, ItemBox.StdItem.Name) = 0) then begin
        New(BoxItem);
        BoxItem.sItemName := sItemName;
        BoxItem.ItemType := TBoxItemType(nItemType);
        BoxItem.nItemCount := nItemCount;
        ItemBox.ItemList.AddObject(ItemBox.StdItem.Name, TObject(BoxItem));
      end;
    end;
  end else begin
    try
      ItemBox.ItemList.SaveToFile(sFileName);
    except

    end;
  end;
  LoadList.Free;
end;

